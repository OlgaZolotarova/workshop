<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="31.0" layoutY="36.0" text="Datasource" />
      <ComboBox fx:id="datasource" layoutX="161.0" layoutY="31.0" onAction="#datasourceSelected" prefHeight="26.0" prefWidth="163.0" />
      <Label layoutX="31.0" layoutY="82.0" text="URL Datasource" />
      <TextField fx:id="url" layoutX="161.0" layoutY="77.0" promptText="Enter database URL" />
      <Label layoutX="31.0" layoutY="125.0" text="Gebruikersnaam" />
      <TextField fx:id="username" layoutX="161.0" layoutY="120.0" promptText="Enter user name" />
      <Label layoutX="31.0" layoutY="167.0" text="Wachtwoord" />
      <TextField fx:id="password" layoutX="161.0" layoutY="162.0" promptText="Enter password" />
      <Button fx:id="connectButton" layoutX="164.0" layoutY="207.0" mnemonicParsing="false" onAction="#connect" text="Set connectie gegevens" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</AnchorPane>
