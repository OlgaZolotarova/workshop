<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
  spacing="5.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <TableView fx:id="table" prefHeight="200.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
      <columns>
        <TableColumn fx:id="voornaamColumn" editable="false" prefWidth="75.0" text="Voornaam" />
        <TableColumn fx:id="achternaamColumn" editable="false" prefWidth="75.0" text="Achternaam" />
        <TableColumn fx:id="tussenvoegselColumn" editable="false" prefWidth="75.0" text="Tussenvoegsel" />
        <TableColumn fx:id="emailColumn" editable="false" prefWidth="75.0" text="e-mail" />
        <TableColumn fx:id="postcodeColumn" editable="false" prefWidth="75.0" text="Postcode" />
        <TableColumn fx:id="huisnummerColumn" editable="false" prefWidth="75.0" text="Huisnummer" />
        <TableColumn fx:id="toevoegingColumn" editable="false" prefWidth="75.0" text="Toevoeging" />
        <TableColumn fx:id="straatnaamColumn" editable="false" prefWidth="75.0" text="Straatnaam" />
        <TableColumn fx:id="woonplaatsColumn" editable="false" prefWidth="75.0" text="Woonplaats" />
      </columns>
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
    <GridPane alignment="TOP_CENTER" hgap="15.0" vgap="5.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <children>
        <Label text="Voornaam" />
        <Label text="Achternaam" GridPane.rowIndex="1" />
        <Label text="Tussenvoegsel" GridPane.rowIndex="2" />
        <TextField fx:id="voornaam" GridPane.columnIndex="1" />
        <TextField fx:id="achternaam" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <TextField fx:id="tussenvoegsel" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <TextField fx:id="email" GridPane.columnIndex="3" />
        <TextField fx:id="postcode" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <TextField fx:id="huisnummer" GridPane.columnIndex="3" GridPane.rowIndex="2" />
        <TextField fx:id="toevoeging" GridPane.columnIndex="3" GridPane.rowIndex="3" />
        <TextField fx:id="straatnaam" GridPane.columnIndex="3" GridPane.rowIndex="4" />
        <TextField fx:id="woonplaats" GridPane.columnIndex="3" GridPane.rowIndex="5" />
        <Label text="e-mail" GridPane.columnIndex="2" />
        <Label text="Postcode" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <Label text="Huisnummer" GridPane.columnIndex="2" GridPane.rowIndex="2" />
        <Label text="Toevoeging" GridPane.columnIndex="2" GridPane.rowIndex="3" />
        <Label text="Straatnaam" GridPane.columnIndex="2" GridPane.rowIndex="4" />
        <Label text="Woonplaats" GridPane.columnIndex="2" GridPane.rowIndex="5" />
      </children>
      <padding>
        <Insets bottom="5.0" left="5.0" top="5.0" />
      </padding>
    </GridPane>
    <ButtonBar prefHeight="40.0" prefWidth="200.0">
      <buttons>
        <Button mnemonicParsing="false" onAction="#newClient" text="New" />
        <Button mnemonicParsing="false" onAction="#saveClient" text="Save" />
        <Button mnemonicParsing="false" onAction="#deleteClient" text="Delete" />
        <Button mnemonicParsing="false" onAction="#generateClients" text="Generate clients" />
      </buttons>
    </ButtonBar>
  </children>
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
  </padding>
</VBox>
